# ============================================
# REQUIRED
# ============================================
OPENAI_SRC_API_KEY="sk-proj-..."
OPENAI_DST_API_KEY="sk-proj-..."

# ============================================
# OPTIONAL (useful if using user-key)
# ============================================
OPENAI_SRC_ORG_ID="org_..."
OPENAI_DST_ORG_ID="org_..."
OPENAI_SRC_PROJECT_ID="proj_..."
OPENAI_DST_PROJECT_ID="proj_..."

# ============================================
# EXECUTION
# ============================================

# Clone mode
# - "all": clone all assistants
# - "by_id": clone only specified IDs
# - "by_name": clone assistants with name containing prefix
CLONE_MODE="all"

# Specific IDs (when CLONE_MODE=by_id)
# Comma-separated: asst_abc123,asst_def456
CLONE_IDS=""

# Name prefix (optional)
# Ex: "PROD - " adds this prefix to destination names
CLONE_NAME_PREFIX=""

# Dry-run (preview only, no actual changes)
DRY_RUN="false"

# ============================================
# OPTIONAL FEATURES
# ============================================

# Clone vector stores and File Search files
INCLUDE_FILE_SEARCH="false"

# Clone Code Interpreter files
INCLUDE_CODE_INTERPRETER="false"

# ============================================
# PERFORMANCE AND LOGS
# ============================================

# Maximum parallel operations
MAX_CONCURRENCY="3"

# Log level: debug | info | warn | error
LOG_LEVEL="info"

# Output directory for reports
OUTPUT_DIR="./out"
